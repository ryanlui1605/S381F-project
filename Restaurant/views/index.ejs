<html>
    <head>
        <title>Login</title>
    </head>
    <body>
        <header>
            <h1>Login</h1>
        </header>
        <form action="/processlogin" method="POST">
            <label>Name</label>
            <input name="username" type="text" required/>
            <br/>
            <label>Password</label>
            <input name="password" type="password"/>
            <br/>
            <input type="submit" value="Log in"/>
        </form>
    </body>
</html>
